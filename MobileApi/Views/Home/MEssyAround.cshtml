<div style="margin-bottom:30px">
    @*<script src="~/Scripts/messingAround_please.js"></script>*@
    <h2>Mobile Database API</h2>
    <br /><br />
    <h4>Upload Database File:</h4>
    <form id="data" method="get" enctype="multipart/form-data">
        <p>Select Database: <select class="databaseTypes" id="databaseTypes" name="dbType"></select></p>
        <input id="uploadFile" type="file" name="uploadFile" />
        <button id="formSubmit">Submit</button>
    </form>
    <div>
        <a id="downloadTemplate" href="..\DataFolder\Template.xlsx" download="Data Template">
            Download Template of Database
        </a>
    </div>
    <br /><br />
    <p>Upload Image Zip Folder:</p>
    <form id="dataImage" method="post" enctype="multipart/form-data">
        <p>Select Database: <select class="databaseTypes" id="imageTypes" name="dbType"></select></p>
        <input id="imageFile" type="file" name="uploadFile" />
        <button id="formImageSubmit">Submit</button>
    </form>
    @* ****ERROR MESSAGE HERE**** *@
    <br />
    <br />
    <p class="statusMessage" id="resultMessage"></p>
    <br />
    <br />

</div>
<script src="~/Scripts/messingAround.js"></script>