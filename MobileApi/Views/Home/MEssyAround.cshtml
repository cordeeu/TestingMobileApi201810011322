<div style="margin-bottom:30px">
    <h2>Mobile Database API</h2>
    <br /><br />

    <h4><font @*color="#1e4d2b"*@ style="font-weight:200;padding-top:100%">Upload Database File:</font></h4>
    <form id="data" method="post" enctype="multipart/form-data">
        <p>Select Database: <select id="databaseTypes" name="dbType"></select></p>
        <input id="uploadFile" type="file" name="uploadFile" />
        <button id="formSubmit">Submit</button>
    </form>

    <br />
    <br />
    <h4>Upload Image Folder:</h4>
    <form id="imageData" method="post" enctype="multipart/form-data">
        <p>Select Database: <select id="imageTypes" name="imageType"></select></p>
        <input id="uploadImages" type="file" name="uploadFile" />
        <button id="imageSubmit">Submit</button>
    </form>

    @* ****ERROR MESSAGE HERE**** *@
    <br />
    <br />
    <p id="errorMessage" style="color:red; font-weight:800">Status: Error Test</p>


    <script src="~/Scripts/messingAround.js"></script>
</div>

